<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.unitconverternew" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity
            android:name=".Krasop"
            android:label="UnitConverterNew" >
            <intent-filter>
                <action android:name="unitconverter.intent.action.GO_KRASOP" /><!--เป็นการจำแนกประเภทการกระทำของintentที่จะใช้งานคำนวณค.ชื้น-->
                <category android:name="android.intent.category.DEFAULT" /><!--เป็นการจำแนกตามหมวดหมู่ที่จะกระทำ-->
            </intent-filter>
        </activity>
    </application>

</manifest>
